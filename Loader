local MoonChicoryHubGamesList = {
    [2888719930] = "Boxing Simulator 2",
}

for i,v in next, MoonChicoryHubGamesList do
    MoonChicoryHubGamesList[i] = table.concat(v:split(' '), '_')
end

if MoonChicoryHubGamesList[game.PlaceId] ~= nil then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Auberon-Coding/MoonChicory/main/Games/" ..game.PlaceId.. ".lua", true))()
else
    return "Game not supported!"    
end
